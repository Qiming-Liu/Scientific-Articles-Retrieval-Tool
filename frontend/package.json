{
  "name": "sar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && next build",
    "start": "next start",
    "export": "next build && next export",
    "test": "jest -c jest.config.js --colors --coverage --passWithNoTests",
    "test:watch": "jest -c jest.config.js --colors --coverage --passWithNoTests --watch",
    "lint:eslint": "eslint --ext .jsx,.js ./pages && eslint --ext .jsx,.js",
    "lint:sass": "sass-lint -v",
    "lint:prettier": "prettier -c .prettierrc \"{pages,store}/**/*.{js,jsx}\" --check",
    "lint": "npm run prettier && npm run lint:eslint && npm run lint:sass && npm run lint:prettier",
    "prettier": "prettier -c .prettierrc \"{pages,store}/**/*.{js,jsx}\" --write"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.4.1",
    "@mui/styles": "^5.4.4",
    "@mui/x-data-grid": "^5.6.1",
    "@redux-devtools/extension": "^3.2.2",
    "@testing-library/user-event": "^14.0.4",
    "axios": "^0.25.0",
    "jest-mock": "^27.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^12.1.0",
    "nextjs-progressbar": "^0.0.14",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.2",
    "react-draggable": "^4.4.4",
    "react-hot-toast": "^2.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "sass": "^1.49.7",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.6",
    "styled-components": "^5.3.3",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@next/eslint-plugin-next": "^12.0.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "sass-lint": "^1.13.1",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
